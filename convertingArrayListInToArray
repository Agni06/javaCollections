public static void(String args[])
{
ArrayList<Integer> fruits  = new ArrayList<>();
fruits.add("Apple");
fruits.add("Banana");
fruits.add("Mango");
fruits.add("Pear");

String[] str  = new String[fruits.size()];

for(int i=0;i<fruits.size();i++)
{
    
   str[i] = fruits.get(i);

}

for(String item : str)
{

 System.out.print(item+" ");   
}

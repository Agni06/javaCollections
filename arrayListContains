public static void(String args[])
{
ArrayList<Integer> al = new ArrayList<>();
al.add(3);
al.add(17);
al.add(6);
al.add(9);
al.add(7);
//searching if 7 is presented in the arrayList.

if(al.contains(7))
{
    System.out.println("Presented");

}
else
{

    System.out.println("Not presented");
}



}
